# Variables de entorno para desarrollo local
# Copia este archivo como .env.local y completa con tus credenciales

# Vercel Postgres (Neon) URLs
# Obtén estas URLs desde: https://vercel.com/dashboard → tu-proyecto → Storage → Postgres → .env.local tab
POSTGRES_URL=""
POSTGRES_PRISMA_URL=""
POSTGRES_URL_NO_SSL=""
POSTGRES_URL_NON_POOLING=""
POSTGRES_USER=""
POSTGRES_HOST=""
POSTGRES_PASSWORD=""
POSTGRES_DATABASE=""

# Twilio WhatsApp Notifications
# Obtén estas credenciales desde: https://console.twilio.com
TWILIO_ACCOUNT_SID=""           # Account SID desde Twilio Console
TWILIO_AUTH_TOKEN=""            # Auth Token desde Twilio Console
TWILIO_WHATSAPP_FROM=""         # Ej: whatsapp:+14155238886 (número de Twilio Sandbox)
TWILIO_WHATSAPP_TO=""           # Ej: whatsapp:+34722224379 (tu número verificado)

